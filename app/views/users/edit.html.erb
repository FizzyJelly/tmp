<h1>Add new user</h1>
<%= form_with(url: user_path(:year), method: :put, local: true) do |form| %>
  <%= form.label "User data" %> <br>
  <%= form.text_field :Name, placeholder: "Enter name.." %><br>
  <%= form.text_field :Surname, placeholder: "Enter surname.." %><br>
  <%= form.text_field :Email, placeholder: "Enter e-mail.." %><br>
  <%= form.text_field :Smail, placeholder: "Enter s-mail.." %><br>
  <%= form.submit %>
<% end %>